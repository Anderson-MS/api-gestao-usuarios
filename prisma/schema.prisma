datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Usuario {
  id             Int      @id @default(autoincrement())
  nome           String
  dataNascimento DateTime
  statusCivil    String
  cpf            String   @unique
  telefone       String
  createdAt      DateTime @default(now())
}
